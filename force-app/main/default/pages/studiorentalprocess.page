        <script>
        $(document).ready(function() {        
            $(".read-more").click(function(){
                $(".teacher-details").toggleClass("show");
            });
            
            var select = document.getElementById('calYearPicker');
            if (select){
                select.innerHTML = '';
                var startYear = new Date().getFullYear() - 118;
                for (var year = startYear; year < startYear + 118; year++) {
                    select.options[select.options.length] = new Option(year, year);
                }
            }
        });
        
        function nextOnFirstScreen(){        
            var selectedCenter = $('[id$=centersel]').val();
            if(!selectedCenter){
                $('.firstCmpErrDiv').removeClass('slds-hide');
                $('.contentDivFirst').html('Please select a location.');
                return;
            }
            
            var slcdResType = $("input[name='options']:checked").val();
            if(!slcdResType){
                $('.firstCmpErrDiv').removeClass('slds-hide');
                $('.contentDivFirst').html('Please select a Resource.');
                return;
            }
            
            var studioTypeOpt = '';
            if(slcdResType != 'Standard'){
                var studioTypeOpt = $("input[name='studioTypeOpt']:checked").val();
                if(!studioTypeOpt){
                    $('.firstCmpErrDiv').removeClass('slds-hide');
                    $('.contentDivFirst').html('Please select a studio.');
                    return;
                }
            }
            
            document.getElementById('secondCmpId').style.display = 'block';
            document.getElementById('firstCmpId').style.display = 'none';
            $('.firstCmpErrDiv').addClass('slds-hide');
            nextFirstScreenAction(studioTypeOpt);  
        }
        function loadintlInputPhone(){
            $(document).ready(function() {   
               $('[id$=inputMobileId]').intlInputPhone(); 
               //$('[id$=inputPhoneId]').intlInputPhone();  
            });    
        }
        function nextOnSecondScreen(){
            var slcdTSlot = $("input[name='tSlotOpt']:checked").val();
            var slcdtcrOpt = $("input[name='teacherOpt']:checked").val();
            
            if(!slcdTSlot){
                $('.secondCmpErrDiv').removeClass('slds-hide');
                $('.contentDivSecond').html('Please select a time slot.');
                return;
            }
            
            document.getElementById('thirdCmpId').style.display = 'block';
            document.getElementById('secondCmpId').style.display = 'none';
            $('.secondCmpErrDiv').addClass('slds-hide');
            nextSecondScreenAction(slcdTSlot);
        }
        function onCompleteShowPaymentInfo(){
            var concatePhoneNoNew = '+' + $('[class$=btn-cc]').html().match(/\d+/);
            concatePhoneNoNew += $('[id$=phoneNumber]').val();
            console.log('%%%%%concatePhoneNoNew-----> '+concatePhoneNoNew);
            document.getElementById('mobId').innerHTML = concatePhoneNoNew;
            document.getElementById('phoId').innerHTML = concatePhoneNoNew;
        }
        function showPaymentInfo(){
            var firstCheck = false;
            // var secondCheck = false;
            $('.inputtxt').each(function(){
                if(!$(this).val()){
                    $(this).parent().parent().addClass('slds-has-error');
                    $(this).parent().next().removeClass('slds-hide');
                    firstCheck = true;
                }else{
                    $(this).parent().parent().removeClass('slds-has-error');
                    $(this).parent().next().addClass('slds-hide');
                }
            });
            
            if(firstCheck)
                return;
                
            /*var mobileNumber = $('[id$=mobilePhn]').val();
            if(!mobileNumber.match(/^\d+/)){
                $('.thirdCmpErrDiv').removeClass('slds-hide');
                $('.contentDivThird').html('Mobile Phone should be in numbers.');
                return;
            }else{
                $('.thirdCmpErrDiv').addClass('slds-hide');
            }*/
            
            var emailVal = $('[id$=emailtxt]').val();
            var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;

            if (reg.test(emailVal) == false){
                $('.thirdCmpErrDiv').removeClass('slds-hide');
                $('.contentDivThird').html('Invalid email address.');
                return;
            }else{
                $('.thirdCmpErrDiv').addClass('slds-hide');
            }
            
            if(!(firstCheck)){
                showPayInfo();
                
                document.getElementById('fourthCmpId').style.display = 'block';
                document.getElementById('thirdCmpId').style.display = 'none'; 
            }
        }
    </script>
        
        

