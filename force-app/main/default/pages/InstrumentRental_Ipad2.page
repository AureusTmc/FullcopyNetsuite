        <script>
            $(document).ready(function() { 
                <apex:variable value="{!1}" var="stuIndex"/>
                <apex:repeat value="{!enrModelList}" var="enrTrial">
                    var tSlot = '{!enrTrial.slcdFullSlot}';//alert(tSlot);
                    var radioName = 'timeSlotRadio'+{!stuIndex};
                    $("input[name='"+radioName+"'][value='"+tSlot+"']").attr('checked', 'checked');
                
                    <apex:variable value="{!stuIndex+1}" var="stuIndex"/>
                </apex:repeat>

            }); 
            
            function enablebtn(){
            var listsize = '{!enrModelList.size}';
            var listsize = Number(listsize);
             var isSuccess = true;
                    for(var i=1; i<= listsize; i++){
                        var option_index = 'timeSlotRadio'+i;
                        var isSelectedTime = $("input[name='"+option_index+"']:checked").val();
                        if(!isSelectedTime){
                          isSuccess = false;   
                        }
                    }
                       
                   if(isSuccess){
                        $("#next_step3").addClass("btn-yellow").removeClass("btn-gray").css("pointer-events", "auto");  
                   }else{
                        $("#next_step3").addClass("btn-gray").removeClass("btn-yellow").css("pointer-events", "none");
                        } 
              
              
            }
        
            function setDuration(stuIndx){
                var optIndx = 'options'+stuIndx;
                var slcdDuration = $("input[name='"+optIndx+"']:checked").val();
                setDurationForStu(stuIndx,slcdDuration);
            }    
        
            function setTimeSlot(stuIndx, slcdSlotDate){
                var optIndx = 'timeSlotRadio'+stuIndx;
                var slcdDuration = $("input[name='"+optIndx+"']:checked").val();
                setTimeslotandgetTchrs(stuIndx,slcdDuration, slcdSlotDate);
            }    
        
            function nextBtn(){
                var enrListSize = {!enrModelList.size};
                var i;
                for (i = 1; i <= enrListSize; i++) {
                    var optIndx = 'timeSlotRadio'+i;
                    var slcdSlot = $("input[name='"+optIndx+"']:checked").val();
                    if(!slcdSlot){
                        //alert('Please select time slot.');
                        $('.cmpErrDiv').removeClass('slds-hide');
                        $('.contentDiv').html('Please select time slot.');
                        return;
                    }
                }    
                $('.cmpErrDiv').addClass('slds-hide');
                proceedNext();
            }
        </script>
        
