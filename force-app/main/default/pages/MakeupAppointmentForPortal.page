                var slcdTSlot = '';
                console.log($('.morning').is(':visible'));
                
                if(typeof $("input[name='tSlotOpt']:checked").val() !== "undefined"){
                    slcdTSlot = $("input[name='tSlotOpt']:checked").val();
                }else{
