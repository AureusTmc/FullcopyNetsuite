            }
            
            function backOnSecondScreen(prcType, slcdListSize, recIndex){
                var tempLstSize  = parseInt(slcdListSize);
                var temprecIndex = parseInt(recIndex);
                
                if(prcType == 'true' && tempLstSize  > 1 && recIndex > 0){
