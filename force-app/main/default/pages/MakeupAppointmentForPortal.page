            function checkorunchecked(obj,enrolId){
                var flag;
                if($(obj).prop('checked') == true){
                    flag = true;
                }else{
                    flag = false;
                }
                
                var count = $('input.checkboxes:checked').length;
                if(count>1){
                    $('#bookPreId').css('display','block');
                }else{
                    $('#bookPreId').css('display','none');
                }
                
                //console.log(flag);
                updateFlag(enrolId,flag);
            }           
            function checkduplicateenrolment(){
                var arr = [];
                var flag = true;               
                $('.checkboxes').each(function(){
                    if($(this).is(':checked')){
                        arr.push($(this).val());
                    }
                });
                //console.log(arr);
                for ( var i = 0; i < arr.length; i++){
                    for (var j = i+1; j< arr.length; j++){                    
                        if (arr[i] === arr[j]){                    
                            flag = false;
                        }
                    }                
                }
                var count = $('input.checkboxes:checked').length;
                var prcType = $("input[name='Processtype']:checked").val();
                if(!(prcType) && count > 1){
                    $('.secondCmpErrDiv').removeClass('slds-hide');
                    $('.contentDivSecond').html('Please select Lesson option type.');    
                }else if($('.checkboxes:checked').length == 0){
                    $('.secondCmpErrDiv').removeClass('slds-hide');
                    $('.contentDivSecond').html('Please select atleast one student.');
                }else{
                    $('.secondCmpErrDiv').addClass('slds-hide');
                    if(flag == false){
                        $('.secondCmpErrDiv').removeClass('slds-hide');
                        $('.contentDivSecond').html('Please select unique student.');
                    }else{
                        $('.secondCmpErrDiv').addClass('slds-hide');
                        selectedStudent(prcType);
                        scrollTop();
                    }
                }
                //console.log('flag = '+flag);
            }
            /*function checkAvlMakeUpTime(){
                validateAvlMakeUnit();
            }*/
            
            function saveCurrentRec(){
                var checkSelOpt = false;                
                $('.inputsel').each(function(){                    
                   if(!$(this).val()){
                       $(this).css('border','2px solid #C23934');
                       checkSelOpt = true;
                   }else{
                       $(this).css('border','1px solid #ddd');
                   }
                });                
                if(checkSelOpt){
                    window.scrollTo(0,0);
                    return;
                }
                var slcdTSlot = '';
                if($('.morning').is(':visible')){
                    slcdTSlot = $("input[name='tSlotOpt']:checked").val();
                }else{
                    slcdTSlot = $('.alltimeslotlist').val();
                }
                console.log(slcdTSlot);
                if(!slcdTSlot){
                    $('.secondCmpErrDiv').removeClass('slds-hide');
                    $('.contentDivSecond').html('Please select a time slot.');
                    window.scrollTo(0,0);
                    return;
                }else{
                    $('.secondCmpErrDiv').addClass('slds-hide');
                }
                saveCurrentRecord(slcdTSlot);
            }
        
            function  saveAppointment(prcType, slcdListSize, recIndex){
                var checkSelOpt = false;                
                $('.inputsel').each(function(){                    
                   if(!$(this).val()){
                       $(this).css('border','2px solid #C23934');
                       //$(this).parent().parent().next().removeClass('slds-hide');
                       checkSelOpt = true;
                   }else{
                       $(this).css('border','1px solid #ddd');
                       //$(this).parent().parent().next().addClass('slds-hide');
                   }
                });                
                if(checkSelOpt){
                    window.scrollTo(0,0);
                    return;
                }
                //Added by Rajesh, 22nd Oct 2019. Add validation for select duplicate teacher when process type is 'Lessons at Same Time'
                var slcdTchr = $('input.checkboxes:checked').length;
                var prcLessType = $("input[name='Processtype']:checked").val();
                if(slcdTchr > 1 && prcLessType == 'Lessons at the Same Time'){
                    var checkDuplicateTchr = false;
                    var mySet = new Set();
                    $('.teacherselect').each(function(){
                        var slctdTchr = $(this).val();                      
                        if(mySet.has(slctdTchr)){
                            checkDuplicateTchr = true;
                            return false; 
                        }
                        mySet.add(slctdTchr);
                    });
                    
                    if(checkDuplicateTchr){
                        $('.secondCmpErrDiv').removeClass('slds-hide');
                        $('.contentDivSecond').html('You can\'t book the same Teacher for \'Lessons at the Same Time\'.');
                        window.scrollTo(0,0);
                        return;
                    }else{
                        $('.secondCmpErrDiv').addClass('slds-hide');
                    }
                }
                
                var slcdTSlot = '';
                if($('.morning').is(':visible')){
                    slcdTSlot = $("input[name='tSlotOpt']:checked").val();
                }else{
                    slcdTSlot = $('.alltimeslotlist').val();
                }
                console.log(slcdTSlot);
                if(!slcdTSlot){
                    $('.secondCmpErrDiv').removeClass('slds-hide');
                    $('.contentDivSecond').html('Please select a time slot.');
                    window.scrollTo(0,0);
                    return;
                }else{
                    console.log('####');
                    $('.secondCmpErrDiv').addClass('slds-hide');
                    //refSummary(slcdTSlot);
                    
                }
                
                var tempLstSize  = parseInt(slcdListSize);
                var temprecIndex = parseInt(recIndex);
                
                console.log(prcType);
                console.log(tempLstSize  > 1);
                console.log(slcdListSize != temprecIndex);
                
                if(prcType == 'true' && tempLstSize  > 1 && ((slcdListSize-1) != temprecIndex)){
                    saveCurrentRecord(slcdTSlot);
                    scrollTop();
                }else{
                    validateAvlMakeUnit();
                    scrollTop();
                }
            }    
            function onCompleteNext(){
                document.getElementById('summCmpId').style.display = 'block';
                $('.second-step').css('display','none');             
            }
            function onPrevious(){
                document.getElementById('summCmpId').style.display = 'none';
                $('.second-step').css('display','block');     
