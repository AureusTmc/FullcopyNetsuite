            <apex:actionFunction name="saveCurrentRecord" action="{!saveCurrentRecord}" reRender="selectedStudentRefresh,loadCalendarPanel,loadCalendarPanel1"  oncomplete="loadCalendarOnTchrSelect();" status="spinner">
                <apex:param name="slcdSlot" value="xy" assignTo="{!slcdSlot}"/>
