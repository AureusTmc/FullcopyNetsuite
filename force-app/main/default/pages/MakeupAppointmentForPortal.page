            </apex:actionStatus>
            <apex:actionFunction name="changeTcr" action="{!changeTchr}" reRender="selectedStudentRefresh,selectedStudentRefresh1,selectedStudentRefresh2,loadCalendarPanel,loadCalendarPanel1" status="spinner">
                <apex:param name="indexno" value="xy" assignTo="{!indexno}"/>
            </apex:actionFunction>    
            <apex:actionFunction name="updateFlag" action="{!updateFlagValue}" reRender="xyz" status="spinner">
                <apex:param name="enrollmentId" value="xy" assignTo="{!enrollmentId}"/> 
                <apex:param name="flag" value="xy" assignTo="{!assignFlagValue}"/>
            </apex:actionFunction>
            <apex:actionFunction name="getTimeSlot" action="{!fetchTimeSlot}"  reRender="loadCalendarPanel,loadCalendarPanel1" oncomplete="loadCalendarOnTchrSelect();" status="spinner"/>               
            <apex:actionFunction name="selectedStudent" action="{!selectedStudent}" reRender="selectedStudentRefresh,selectedStudentRefresh1,selectedStudentRefresh2" status="spinner" oncomplete="$('.first-step').addClass('slds-hide');$('.second-step').css('display','block');">            
