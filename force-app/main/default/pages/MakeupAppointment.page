                                                                                <apex:repeat value="{!tSlotMap.morningSlot}" var="ins">
                                                                                    <span class="slds-button slds-radio_button">
                                                                                        <input name="tSlotOpt" type="radio" id="{!ins}" value="{!ins}"/>
