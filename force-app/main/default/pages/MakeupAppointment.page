                                     <div class="slds-col slds-size_1-of-1 slds-p-horizontal_medium" style="text-align: center;padding: 50px;">
                                         <label class="slds-form-element__label field-title" for="form-element-help-01" style="font-size: 18px;">Your makeup Lesson has been scheduled successfully.
                                         </label>
                                     </div>
                                 </div>
                            </apex:outputPanel>
                            
                            
                            <div class="firstCmpcls" id="firstCmpId">
                                <div class="content-body slds-col">
                                    <div class="slds-form-element">
                                        
                                        <div class="slds-notify_container slds-is-relative slds-hide firstCmpErrDiv">
                                            <div class="slds-notify slds-notify_toast slds-theme_error slds-m-left_none slds-m-right_medium" role="status">
                                                <div class="slds-notify__content">
                                                    <h2 class="slds-text-heading_small contentDivFirst"></h2>
                                                </div>
                                                
                                                <div class="slds-notify__close">
                                                    <button class="slds-button slds-button_icon slds-button_icon-inverse" onclick=" $('.firstCmpErrDiv').addClass('slds-hide');return false;" title="Close">
                                                        X
                                                        <span class="slds-assistive-text">Close</span>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <apex:outputPanel id="refreshFrm" rendered="{!!isSuccess}">
                                            <div class="slds-form-element">
                                                <div class="slds-col slds-size_1-of-1 slds-p-horizontal_medium">
                                                    <label class="slds-form-element__label field-title" for="form-element-help-01">Parent Details</label>    
                                                </div>
                                                <div class="slds-grid slds-wrap slds-p-horizontal_medium">
                                                    <div class="slds-col dark-bg billig-info slds-grid">
                                                        <ul class="slds-grid slds-wrap slds-size_1-of-1">
                                                            <li class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_medium">
                                                                <div><strong>Parent Name</strong> {!parName} </div></li>
                                                            <li class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_medium">
                                                                <div><strong>Make-up Mins</strong> {!makeAvlMinutes}</div></li>
                                                            <li class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_medium">
                                                                <div><strong>Select Student</strong>
                                                                    <div class="slds-form-element__control">
                                                                        <div class="slds-select_container">
                                                                            <apex:selectList styleClass="slds-select inputsel" multiselect="false" onchange="changeStu();return false;" size="1" value="{!slcdStu}"> 
                                                                                <apex:selectOptions value="{!stuOptions}"/>
                                                                             </apex:selectList>
                                                                        </div>
                                                                        <div class="slds-form-element__help slds-hide errortxt">This field is required</div>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                            <li class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_medium">
                                                                <div style="line-height: 28px;"><strong>Center Name</strong>&nbsp;<span id="cntrName">{!CenterName}</span> </div></li>
                                                            <!-- Updated by Rajesh: 09-7-2019, For add instrument filter -->
                                                            <li class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_medium">
                                                                <div><strong>Select Instrument</strong>
                                                                    <div class="slds-form-element__control">
                                                                        <div class="slds-select_container">
                                                                            <apex:selectList styleClass="slds-select" multiselect="false" onchange="refrechTchr();return false;" value="{!slcdInstrument}" size="1"> 
                                                                                <apex:selectOptions value="{!Instruments}"/>
                                                                             </apex:selectList>
                                                                        </div>
                                                                        <div class="slds-form-element__help slds-hide errortxt">This field is required</div>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                            <li class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_medium">
                                                                <div><strong>Select Teacher</strong>
                                                                    <div class="slds-form-element__control">
                                                                        <div class="slds-select_container">
                                                                            <apex:selectList styleClass="slds-select inputsel" multiselect="false" size="1" onchange="changeTcr();" value="{!slcdTchr}"> 
                                                                                <apex:selectOptions value="{!Teachers}"/>
                                                                            </apex:selectList>
                                                                        </div>
                                                                        <div class="slds-form-element__help slds-hide errortxt">This field is required</div>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                            <li class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_medium">
                                                                <div><strong>Select Package</strong>
                                                                    <div class="slds-form-element__control">
