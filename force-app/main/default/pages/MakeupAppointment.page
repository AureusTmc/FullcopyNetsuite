                                                                            <apex:selectList styleClass="slds-select inputsel" multiselect="false" size="1" onchange="getTimeSlot();" value="{!slcdPkg}"> 
                                                                                <apex:selectOptions value="{!MakeupPkg}"/>
                                                                            </apex:selectList>
                                                                        </div>
                                                                        <div class="slds-form-element__help slds-hide errortxt">This field is required</div>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                            
                                             <!-- Datepicker -->
                                            <apex:outputPanel rendered="{!NOT(ISNULL(slcdPkg))}">
                                                <div class="slds-grid slds-wrap">
                                                    <div class="slds-col slds-size_1-of-1 slds-p-horizontal_medium"><label class="slds-form-element__label field-title" for="form-element-help-01">Time Available</label></div>
                                                    
                                                    <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_medium">
                                                        <apex:outputPanel id="datePickPanel">
                                                            <div id="datepicker" class=""></div>
                                                        </apex:outputPanel>       
                                                    </div>
                                                    
                                                    <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_medium">
