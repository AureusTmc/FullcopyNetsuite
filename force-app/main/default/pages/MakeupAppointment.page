<apex:page id="pg" showHeader="false" standardStylesheets="true" sidebar="false" docType="html-5.0" Controller="MakeupAppointmentCntrl" applyBodyTag="False" applyHtmlTag="False">
    <head>
        <title>Aureus Academy â€“ Makeup Appointment</title>
        <!--<apex:slds /> -->
        <apex:stylesheet value="{!URLFOR($Resource.SLDS283, '/styles/salesforce-lightning-design-system.min.css')}"/>
        <!-- Updated by Rajesh, 31st July 2019: To Solve increase field length when click in inputfield in mobile devices-->
        <!--<meta name="viewport" content="width=device-width, initial-scale=1.0"/>-->
        <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0"/>
        <apex:stylesheet value="{!URLFOR($Resource.Jquery_plugin, '/Jquery_plugin/jquery-ui.css')}"/>
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous" />
        <apex:includeScript value="{!URLFOR($Resource.Jquery_plugin, '/Jquery_plugin/jquery/jquery.js')}"/>
        <apex:includeScript value="{!URLFOR($Resource.Jquery_plugin, '/Jquery_plugin/jquery-ui.js')}"/>
        
        <script>
        
            $(document).ready(function() { 
                // loadCalendarOnTchrSelect();       
            }); 
        
            function  saveAppointment(){
                var checkSelOpt = false;
                
                $('.inputsel').each(function(){
                   if(!$(this).val()){
                       $(this).parent().parent().parent().parent().addClass('slds-has-error');
                       $(this).parent().parent().next().removeClass('slds-hide');
                       checkSelOpt = true;
                   }else{
                       $(this).parent().parent().parent().parent().removeClass('slds-has-error');
                       $(this).parent().parent().next().addClass('slds-hide');
                   }
                });
                
                if(checkSelOpt){
                    window.scrollTo(0,0);
                    return;
                }
                
                //Added by Rajesh 26th june 2019, Added centre validation temprary purpose
                //Added by Rajesh 8th July 2019, allow for 'Aureus Rochester' centre, as per Ashish discussion 
                //Added by Rajesh 10th July 2019, allow for 'Aureus AMK Hub' and 'Aureus United Square' centre, as per Ashish discussion 
                //Added by Rajesh 17th July 2019, allow for 'Aureus Northpoint' and 'Aureus Forum' centre, as per Ashish discussion 
                var cntrName = $('#cntrName').text();
                if(cntrName == "Aureus 112 Katong" || cntrName == "Aureus Eastpoint Mall" || cntrName == "Aureus Suntec" || cntrName == "Aureus Rochester" || cntrName == "Aureus AMK Hub" || cntrName == "Aureus United Square" || cntrName == "Aureus Northpoint" || cntrName == "Aureus Forum"  || cntrName == "Aureus Westgate"){
                    $('.firstCmpErrDiv').addClass('slds-hide');
                }else{                    
                    $('.firstCmpErrDiv').removeClass('slds-hide');
                    // $('.contentDivFirst').html('This facility is not available on this centre. Please contact Centre Manager.');
                    $('.contentDivFirst').html('The centre you have selected is currently not accepting make-up bookings from the online interface. We estimate it will be available from mid-July and we apologise for the inconvenience caused. Please contact the centre and we will be happy to assist you.');
                    window.scrollTo(0,0);
                    return;
                } 
                
                var slcdTSlot = $("input[name='tSlotOpt']:checked").val();
                if(!slcdTSlot){
                    $('.firstCmpErrDiv').removeClass('slds-hide');
                    $('.contentDivFirst').html('Please select a time slot.');
                    window.scrollTo(0,0);
                    return;
                }else{
                    $('.firstCmpErrDiv').addClass('slds-hide');
                } 
                
                                
                validateAvlMakeUnit();
            }    
            
            function onCompleteNext(){
                document.getElementById('summCmpId').style.display = 'block';
                document.getElementById('firstCmpId').style.display = 'none';               
            }
        
            function saveChanges(){
                //Added by Rajesh 26th June 2019, for disable booking button on click on submit button
                disableBookBtn(true);
                
                var slcdTSlot = $("input[name='tSlotOpt']:checked").val();
                saveMakeupApp(slcdTSlot);
            }
            
            
            
             //Added by Rajesh 26th June 2019, for enable/disable booking button for stop double click
            function disableBookBtn(isDisable){
                if(isDisable)
                    $('#cnfrmBtnId').css('pointer-events', 'none');
                else
                    $('#cnfrmBtnId').css('pointer-events', '');
            }
        </script>        
    </head>
    
    
    <body class="slds-scope ">
        
        <apex:form id="frm" styleClass="slds-col">
            <apex:actionStatus id="spinner">
                <apex:facet name="start"> 
                    <apex:outputPanel id="backdrop" styleClass="popupBackground">
                        <div role="status" class="slds-spinner slds-spinner_brand slds-spinner_large"> 
                            <div class="slds-spinner__dot-a"></div>
                            <div class="slds-spinner__dot-b"></div>
                        </div>
                    </apex:outputPanel>
                </apex:facet>
            </apex:actionStatus>
            
            <div class="main-content">
                <div class="slds-grid slds-wrap slds-grid--align-center">
                    <div class="slds-col content-box">  
                        <div class="header slds-col"><div class="slds-col"><a href="#" class="logo"><img src="https://aureusacademy.com/wp-content/uploads/2017/03/cropped-Aureus-Logo-Small.png"/></a></div>
                            <div class="slds-col ">
                                <span class="main-title">Make-up Lesson</span>
                            </div>
                        </div>
                        
                        <apex:outputPanel id="loadCalendarPanel">
                            <apex:outputPanel id="loadCalendarPanel1">
                                <script>                
                                    var slot;
                                    
                                    function loadCalendarOnTchrSelect(){ 
                                        var tSl = '{!slcdSlot}';                                            
                                        if(tSl){
                                            $("input[name='tSlotOpt'][value='"+tSl+"']").attr('checked', 'checked');
                                        }
                                        
                                        slot = {!tSlotStr};  
                                        
                                        $( "#datepicker" ).datepicker({
                                            inline: true,
                                            minDate:0,
                                            dateFormat: 'd/m/yy',
                                            beforeShowDay: highlightDays,
                                            defaultDate : '{!slcdDate}',
                                            onSelect: function(dateText, inst) {
                                                refreshSlotsTcr(dateText);
                                            }
                                        });
                                    }
                                    
                                    function highlightDays(date) {
                                        //alert('sdfsdf');
                                        var st = date.getDate()+'/'+(date.getMonth()+1)+'/'+date.getFullYear();
                                        
                                        if(slot[st]){
                                            var isMrgAvl = slot[st].isAvlMrgSlot;
                                            var isAfternoonAvl = slot[st].isAvlAfternoonSlot;  
                                            
                                            if(isMrgAvl && isAfternoonAvl){
                                                return [true, 'day-availability--morning day-availability--afternoon'];
                                            }else if(isMrgAvl){
                                                return [true, 'day-availability--morning'];
                                            }else if(isAfternoonAvl){
                                                return [true, 'day-availability--afternoon'];
                                            }
                                        }
                                        
                                        return [true, 'ui-datepicker-unselectable ui-state-disabled'];
                                    }
                                    
                                    //Added by Rajesh 26th June 2019, for enable button if any error
                                    function checkError(){
                                        var isErrOnSave = '{!isError}';
                                        
                                        if(isErrOnSave == 'true'){
                                            disableBookBtn(false);
                                        }
                                    }
                                </script>
                                
                            </apex:outputPanel>
                        </apex:outputPanel>
                        <apex:outputPanel id="validateMakeUnit">
                            <script>
                                function checkAvlMakeError(){
                                    var isErrOnAvlMake = '{!isError}';
                                    
                                    if(isErrOnAvlMake == 'true'){
                                        $('.firstCmpErrDiv').removeClass('slds-hide');
                                        $('.contentDivFirst').html('{!errorMsg}');
                                        
                                        window.scrollTo(0,0);
                                    }else{
                                        $('.firstCmpErrDiv').addClass('slds-hide');
                                        
                                        var slcdTSlot = $("input[name='tSlotOpt']:checked").val();
                                        refSummary(slcdTSlot);                                        
                                    }
                                }
                            </script>
                        </apex:outputPanel> 
                        <!-- Updated by Rajesh: 09-7-2019, For add instrument filter -->    
                        <!--<apex:actionFunction name="refrechTchr" action="{!changeStudent}" reRender="refreshFrm" status="spinner"/>-->
                        <apex:actionFunction name="changeStu" action="{!changeStudent}" reRender="refreshFrm" status="spinner"/>
                        <apex:actionFunction name="refrechTchr" action="{!changeInstrument}" reRender="refreshFrm" status="spinner"/>
                        <apex:actionFunction name="changeTcr" action="{!changeTchr}" reRender="completeFrm" status="spinner"/>
                        <apex:actionFunction name="getTimeSlot" action="{!fetchTimeSlot}"  reRender="refreshFrm,loadCalendarPanel,loadCalendarPanel1" oncomplete="loadCalendarOnTchrSelect();" status="spinner"/>
                        <apex:actionFunction name="refreshSlotsTcr" action="{!fetchTimeSlot}" reRender="refreshTimeSlot" status="spinner">
                            <apex:param name="slcdDt" value="xy" assignTo="{!slcdDate}"/> 
                        </apex:actionFunction>
                        <apex:actionFunction name="validateAvlMakeUnit" action="{!checkAvlMakeup}" reRender="validateMakeUnit" oncomplete="checkAvlMakeError();return false;" status="spinner"/>
                        <apex:actionFunction name="refSummary" action="{!refreshSummary}" oncomplete="onCompleteNext();" reRender="SummaryScreenPanel" status="spinner">
                            <apex:param name="slcdSlot" value="xy" assignTo="{!slcdSlot}"/>
                        </apex:actionFunction>
                        <apex:actionFunction name="saveMakeupApp" action="{!saveBooking}" oncomplete="checkError();" reRender="completeFrm,loadCalendarPanel,loadCalendarPanel1" status="spinner">
                             <apex:param name="slcdSlot" value="xy" assignTo="{!slcdSlot}"/> 
                        </apex:actionFunction>
                        
                        <apex:outputPanel id="completeFrm">
                            <apex:outputPanel rendered="{!isError}" >
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-col slds-size_1-of-1 slds-p-horizontal_medium" style="text-align: center;padding: 50px;">
                                        <label class="slds-form-element__label field-title" for="form-element-help-01" style="font-size: 18px;">{!errorMsg}
                                        </label>
                                    </div>
                                </div>
                            </apex:outputPanel>
                             <apex:outputPanel id="successMsg" rendered="{!isSuccess}" >
