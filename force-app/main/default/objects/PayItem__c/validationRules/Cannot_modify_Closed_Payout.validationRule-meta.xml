<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_modify_Closed_Payout</fullName>
    <active>true</active>
    <errorConditionFormula>AND($Profile.Name!=&apos;System Administrator&apos; ,ISPICKVAL(Payout__r.Status__c,&apos;Closed&apos;))</errorConditionFormula>
    <errorMessage>Closed Payout cannot be modified.</errorMessage>
</ValidationRule>
