<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Promotion_Discount__c</fullName>
    <externalId>false</externalId>
    <formula>AND(
   NOT(ISBLANK(TEXT(Promotion_period_In_months__c))),
   Promotion_Discount__c &gt; 0,
   NOT(ISBLANK(Package_after_Promotion__c))
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Promotion Discount</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
