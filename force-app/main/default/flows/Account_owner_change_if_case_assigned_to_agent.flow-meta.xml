        <expression>AND(ISCHANGED({!myVariable_old.OwnerId}) ,

BEGINS({!myVariable_old.OwnerId}, &quot;005&quot;))</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND      (
               ISCHANGED([Case].Status),
		ISPICKVAL(PRIORVALUE([Case].Status),&apos;New&apos;), 
		BEGINS( [Case].OwnerId, &quot;00G&quot;)
		)
|| 
	
	AND
        (
	ISCHANGED([Case].Stage__c ),
	ISPICKVAL(PRIORVALUE([Case].Stage__c), &apos;Trial Requested&apos;),
	[Case].RecordType.DeveloperName = &apos;Sales_Enquiry&apos;,
	BEGINS( [Case].OwnerId, &quot;00G&quot;),
	NOT(CONTAINS([Case].LastModifiedBy.LastName, &quot;Site&quot; ))
	)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>AND      (
               ISCHANGED({!myVariable_current.Status}),
		ISPICKVAL(PRIORVALUE({!myVariable_current.Status}),&apos;New&apos;), 
		BEGINS( {!myVariable_current.OwnerId}, &quot;00G&quot;)
		)
|| 
	
	AND
        (
	ISCHANGED({!myVariable_current.Stage__c} ),
	ISPICKVAL(PRIORVALUE({!myVariable_current.Stage__c}), &apos;Trial Requested&apos;),
	{!myVariable_current.RecordType.DeveloperName} = &apos;Sales_Enquiry&apos;,
	BEGINS( {!myVariable_current.OwnerId}, &quot;00G&quot;),
	NOT(CONTAINS({!myVariable_current.LastModifiedBy.LastName}, &quot;Site&quot; ))
	)</expression>
    </formulas>
