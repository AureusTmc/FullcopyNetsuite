    <description>Exculded one account id from update action: 4 May 2020</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(ISCHANGED([Case].OwnerId) ,

BEGINS([Case].OwnerId, &quot;005&quot;))

</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
