                    function releaseBlock(){
                        $('#releaseBookingReasonError').addClass('slds-hide');
                        $('#releaseBookingReason').empty();
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.AureusCalendarController.getPickListValues}',
