    function getInstruments(filterTeacherList){
        //console.log('filterTeacherList',filterTeacherList);
        $('#instruments').empty();
        Visualforce.remoting.Manager.invokeAction(
            '{!$RemoteAction.AureusCalendarController.getPickListValues}',
