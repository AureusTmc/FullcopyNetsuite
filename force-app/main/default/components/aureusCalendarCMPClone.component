        function toJSDate( dateTime ) {
            //2019-02-09T16:15:00
            var dateTime = dateTime.split("T");//dateTime[0] = date, dateTime[1] = time
            
            var date = dateTime[0].split("-");
            var time = dateTime[1].split(":");
           
            return new Date(date[0], (date[1]-1), date[2], time[0], time[1], 0, 0);
            
        }
